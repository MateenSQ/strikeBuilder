@model IEnumerable<Strike>

<h1>Select a Move</h1>

<a asp-action="Index">< Back</a>

<hr />

<div class="strikeContainer">
    @foreach (var item in Model)
    {
        <form method="post" asp-action="Select" asp-route-id="@item.Id">
            <button type="submit" class="selectStrike">
                <h3>@item.Name</h3>
                <p>@item.ShortDescription</p>
            </button>
        </form>
    }
</div>